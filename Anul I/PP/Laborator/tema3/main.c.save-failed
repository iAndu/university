#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    FILE *in = fopen("date.in", "r");
    FILE *out = fopen("date.out", "w");

    char *str = malloc(256 * sizeof(char));
    fprintf(out, "Cuvintele care contin cratima sunt :\n");
    while (fscanf(in, "%s ", str)) {
        if (strchr(str, "-")) {
            fprintf(out, "%s ", str);
        }
    }
    return 0;
}
